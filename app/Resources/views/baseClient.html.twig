{% block javascripts %}
    <script src="{{ asset('assets/vendor/jquery/dist/jquery.js')}}"></script>
    <script src="{{ asset('assets/vendor/bootstrap/dist/js/bootstrap.min.js')}}"></script>
    <script src="{{ asset('assets/vendor/jquery.kyco.easyshare/dist/jquery.kyco.easyshare.min.js')}}"></script>
    <script src="{{ asset('js/client/jquery-ui.js')}}"></script>
    <script src="{{ asset('js/client/owl.carousel.js')}}"></script>
    <script src="{{ asset('js/lib/common.js')}}"></script>

    <script type="application/x-javascript">
        addEventListener("load", function() { setTimeout(hideURLbar, 0); }, false); function hideURLbar(){ window.scrollTo(0,1); }
        $("span.menu").click(function(){
            $(".top-menu ul").slideToggle(200);
        });
        $( "#datepicker,#datepicker1" ).datepicker();
        $("#owl-demo").owlCarousel({
            items : 1,
            lazyLoad : true,
            autoPlay : true,
            navigation : true,
//            navigationText :  false,
            navigationText : [
                "<span style='color:black' class='glyphicon glyphicon-chevron-left fa-5x'></span>",
                "<span style='color:black' class='glyphicon glyphicon-chevron-right fa-5x'></span>"
            ],
            pagination : false,
            next : 'nextsqsqsq'
        });
        common().scrollToTop();
        common().menu();
    </script>
{% endblock %}

</body>
</html>