Smartphone Compatible web template, free webdesigns for Nokia, Samsung, LG, SonyEricsson, Motorola web design" />
    {% stylesheets filter='cssrewrite, ?yui_css'
    'assets/vendor/bootstrap/dist/css/bootstrap.min.css'
    'css/font-awesome.css'
    'http://fonts.googleapis.com/css?family=Open+Sans:600italic,700italic,800italic,400,300,600,700,800'
    'http://fonts.googleapis.com/css?family=Pinyon+Script'
    'http://fonts.googleapis.com/css?family=Quicksand:400,700'
    'bundles/indraclient/css/style.css'
    'bundles/indraclient/css/owl.carousel.css'
    'bundles/indraclient/css/jquery-ui.css'
    'assets/vendor/eonasdan-bootstrap-datetimepicker/build/css/bootstrap-datetimepicker.min.css'
    'assets/vendor/bootstrap-multiselect/dist/css/bootstrap-multiselect.css'
    'assets/vendor/flag-icon-css/css/flag-icon.min.css'
    'assets/vendor/jquery-gridder/dist/css/jquery.gridder.min.css'
    'assets/vendor/jquery.kyco.easyshare/dist/jquery.kyco.easyshare.css'
    'css/social.css'

    %}
    <link rel="stylesheet" href="{{ asset_url }}" type="text/css" />
    {% endstylesheets %}
    {#<link href="css/bootstrap.css" rel='stylesheet' type='text/css'/>#}
    {#<link href="css/style.css" rel="stylesheet" type="text/css" media="all"/>#}

</head>
<body>
<!--header starts-->
<div class="header">
    <div class="top-header">
        <div class="container-fluid">
            <div class="row">
                <div class="col-md-3" style="margin-top: 1%;">
                    <span class="label" style="font-size: 20px;">
                        <a style="color:white;" href="{{ path(app.request.attributes.get('_route'), app.request.attributes.get('_route_params')|merge({'_locale': 'en'})) }}"><span style="margin-top: 1%;" class="flag-icon flag-icon-gb"></span> EN</a>
                        <a style="color:white;" href="{{ path(app.request.attributes.get('_route'), app.request.attributes.get('_route_params')|merge({'_locale': 'fr'})) }}"><span class="flag-icon flag-icon-fr"></span> FR</a>
                        <a style="color:white;" href="{{ path(app.request.attributes.get('_route'), app.request.attributes.get('_route_params')|merge({'_locale': 'es'})) }}"><span class="flag-icon flag-icon-es"></span> ESP</a>
                    </span>
                </div>
                <div class="col-md-9">
                    <div class="logo" style="color:white ">
                        <span style="font-size: 1.5em;color: darkorange;" class="glyphicon glyphicon glyphicon-star" aria-hidden="true"></span>
                        <span style="font-size: 1.5em;color: darkorange;" class="glyphicon glyphicon glyphicon-star" aria-hidden="true"></span>
                        <b style="font-family: 'Pinyon Script', cursive;font-size: 2.5em;">INDRA HOTEL</b>
                        <span style="font-size: 1.5em;color: darkorange;" class="glyphicon glyphicon glyphicon-star" aria-hidden="true"></span>
                        <span style="font-size: 1.5em;color: darkorange;" class="glyphicon glyphicon glyphicon-star" aria-hidden="true"></span>


                    {#<a href="index.html"><img width="200px" height="50px" src="{{ asset('bundles/indraclient/images/indra.jpg')}}"/></a>#}
                    </div>
                    <span class="menu"> </span>
                    <div class="m-clear"></div>
                    <div class="top-menu">
                        <ul>
                            <li ><a href="{{ path('indra_client_homepage') }}">ACCUEIL</a></li>
                            <li><a class="scroll" href="restaurant.html">RESTAURANT & BAR</a></li>
                            <li><a class="scroll" href="conference.html">TOURISME</a></li>
                            <li><a class="scroll" href="{{ path('reservationClient_informations') }}">RESERVATION</a></li>
                            <li><a class="scroll" href="booking.html">BLOG</a></li>
                            <li><a class="scroll" href="booking.html">A PROPOS DE NOUS</a></li>
                            <li><a class="scroll" href="contact.html">CONTACTS</a></li>
                        </ul>
                    </div>
                    <div class="clearfix"></div>
                </div>

            </div>

        </div>
    </div>
    {% block banner %}
    {% endblock %}
</div>
{% block body %}{% endblock %}
